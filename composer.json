        "laravel-enso/roles": "3.3.*",
        "laravel-enso/searchable": "1.2.*",
        "laravel-enso/select": "3.2.*",
        "laravel-enso/teams": "2.1.*",
        "laravel-enso/track-who": "1.8.*",
        "laravel-enso/tutorials": "3.2.*",
        "laravel-enso/tables": "3.0.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Core\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
