    "require": {
        "laravel/framework": "^6.0",
        "guzzlehttp/guzzle": "~6.0",
        "jenssegers/date": "^3.4",
        "laravel-enso/action-logger": "2.3.*",
        "laravel-enso/activity-log": "3.0.*",
        "laravel-enso/avatars": "2.4.*",
